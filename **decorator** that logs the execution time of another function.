import time
import functools

# Decorator to log execution time
def log_time(func):
    @functools.wraps(func)   
    def wrapper(*args, **kwargs):
        start = time.time()   
        result = func(*args, **kwargs) 
        end = time.time()     
        print(f"Function '{func.__name__}' executed in {end - start:.6f} seconds")
        return result
    return wrapper

# Example usage
@log_time
def slow_function():
    time.sleep(2)   
    print("Finished work!")

slow_function()
