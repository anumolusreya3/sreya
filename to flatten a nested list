def flatten_list(nested_list):
    flat_list = []
    for item in nested_list:
        if isinstance(item, list):
            for sub_item in item:
                flat_list.append(sub_item)
        else:
            flat_list.append(item)
    return flat_list

# Example
nested = [[1, 2], [3, 4], 5]
flattened = flatten_list(nested)
print(flattened)
