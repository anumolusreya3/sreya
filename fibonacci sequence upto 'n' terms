def fibonacci(n):
    if n <= 0:
        return []
    elif n == 1:
        return [0]
    
    seq = [0, 1] 
    for i in range(2, n):
        next_term = seq[i-1] + seq[i-2]
        seq.append(next_term)
    return seq

# Example
terms = int(input("Enter the number of terms: "))
fib_sequence = fibonacci(terms)
print("Fibonacci sequence:", fib_sequence)
